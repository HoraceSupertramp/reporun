  "bin": "./dist/index.js",
  "scripts": {
    "build:server": "tsc -p ./server/tsconfig.json",
    "build:client": "NODE_ENV=production webpack build --config ./client/webpack.config.js",
    "build": "npm run build:server && npm run build:client",
    "dev:server": "tsnd --respawn ./server/index.ts",
    "dev:client": "NODE_ENV=development webpack watch --config ./client/webpack.config.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\" --kill-others"
  },
